    var themeKey = getInitialTheme();
    var vars = injectedThemeDefinitions[themeKey]; // Note: Any changes here to how we look up the theme page overrides based
    // on the current path may also need to be applied in the theme-provider.

    const rawPathname = window.location.pathname;
    const pathname = rawPathname.replace(/^\/[a-z]{2}_[A-Z]{2}\//, "/");
    const overwritesKey = `/${pathname.split("/")[1]}`;
    var pageOverwrites = injectedThemePageOverwrites[pathname] || injectedThemePageOverwrites[overwritesKey];

    if (pageOverwrites && pageOverwrites[themeKey]) {
      // create a copy to be sure to not mutate the original object
      vars = JSON.parse(JSON.stringify(vars));
      Object.keys(pageOverwrites[themeKey]).forEach(function (key) {
        vars[key] = pageOverwrites[themeKey][key];
      });
    }

    Object.keys(vars).forEach(function (key) {
      document.documentElement.style.setProperty(key, vars[key]);
    });
  }
